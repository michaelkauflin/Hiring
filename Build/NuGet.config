<?xml version="1.0" encoding="utf-8"?>
<!-- 
This config file reconfigures NuGet for tools packages. In order for NuGet to pick it up, 
NuGet.exe MUST be run from the Build folder or one of its descendants.
-->
<configuration>
	<config>
		<add key="repositorypath" value="Tools" />
	</config>
</configuration>